  <VoicesAlerts>
  
	<Alert>
		<Type>SoundOnly</Type>
		<SoundEvent>aural_500ft</SoundEvent>
		<Condition>
			<StateMachine>
				<State id="Under" value="0">
					<Transition to="Over">
						<GreaterEqual>
							<Simvar name="PLANE ALT ABOVE GROUND MINUS CG" unit="feet"/>
							<Constant>510</Constant>
						</GreaterEqual>
					</Transition>
				</State>
				<State id="Over" value="0">
					<Transition to="Alerting">
						<LowerEqual>
							<Simvar name="PLANE ALT ABOVE GROUND MINUS CG" unit="feet"/>
							<Constant>500</Constant>
						</LowerEqual>
					</Transition>
				</State>
				<State id="Alerting" value="1">
					<Transition to="Under">
						<Constant>1</Constant>
					</Transition>
				</State>
			</StateMachine>
		</Condition>
	</Alert>
	
	<Alert>
		<Type>SoundOnly</Type>
